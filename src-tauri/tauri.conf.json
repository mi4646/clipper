{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Clipper",
  "version": "0.1.0",
  "identifier": "com.example.clipper",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "clipper",
        "width": 1401,
        "height": 805,
        "resizable": true,
        "fullscreen": false,
        "maximized": false,
        "visible": true,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets":"all"
  }
}
